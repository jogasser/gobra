package main

import "pkg1"
import "pkg2"

func main () {
  r@ := pkg2.Rect{width: 2, height: 3}
  c@ := pkg2.Circle{radius: 10}
  fold r.mem()
  fold c.mem()

  test(Area(&r), Area(&c))
}

requires acc(s.mem()) && s.Size()
ensures ret > 0
func Area(s pkg1.Shape) (ret int) {
  return s.Area()
}


